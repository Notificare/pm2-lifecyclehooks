{
  "name": "pm2-lifecyclehooks",
  "version": "0.0.1",
  "description": "A module to handle AWS lifecycle hooks for PM2",
  "dependencies": {
    "@aws-sdk/client-auto-scaling": "^3.238.0",
    "axios": "^1.2.2",
    "pm2": "^5.2.2",
    "pmx": "^1.6.7"
  },
  "config": {
    "workers_to_monitor": [
      "test-app"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clikex/pm2-lifecyclehooks.git"
  },
  "apps": [
    {
      "name": "pm2-lifecyclehooks",
      "script": "src/main.ts",
      "max_memory_restart": "200M"
    }
  ],
  "author": "Wouter van der Meulen",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  }
}
